#init(["OAuth 2.0", "应用授权申请"], "token_managment")
        <div class="row">
         <form id="form" action="/client/create" method="post">
         <input type="hidden" id="id" name="id" value="$!{client.id}">
         <input type="hidden" id="clientId" name="clientId" value="$!{client.clientId}">
         <input type="hidden" id="clientSecret" name="clientSecret" value="$!{client.clientSecret}">
          <div class="col-lg-12 col-sm-12 col-xs-12">
            <div class="widget">
              <div class="widget-body padding-top-20">
                  <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                        <div class="form-group margin-bottom-10">
							<label for="input-company" class="col-md-2 control-label no-padding-right text-nowrap">应用名称：</label>
							<div class="col-md-10">
								<input type="text" name="clientName" value="$!{client.clientName}" class="form-control" />
							</div>
						</div>
                    </div>
				  </div>
				  
				  <div class="row margin-top-10">
					  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
						  <div class="form-group">
							  <div class="buttons-preview col-sm-offset-1 col-sm-11" align="center"> 
							  	<button type="submit" class="btn btn-darkorange">确认提交</button>
							  	<a href="javascript:void(0);" onclick="javascript:history.go(-1);" class="btn btn-default shiny">取消返回</a>
							  </div>
						  </div>
					  </div>
				  </div>
              </div>
            </div>
          </div>
          </form>
        </div>
    